            <profile id="447c-82b5-1aa0-190d::513d-39e0-1961-cfe5" name="Archmagos" publicationId="ebe8-544e-pubN65537" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">
